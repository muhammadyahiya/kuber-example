Launch a t3 small instance
ssh to the instance
curl -LO https://storage.googleapis.com/kubernetes-release/release/`curl -s https://storage.googleapis.com/kubernetes-release/release/stable.txt`/bin/linux/amd64/
kubectl && chmod +x ./kubectl && mv ./kubectl /usr/bin/
kubectl version --client
sudo yum install docker -y
curl -Lo minikube https://storage.googleapis.com/minikube/releases/latest/minikube-linux-amd64 && chmod +x minikube && sudo mv minikube /usr/bin/
minikube version
yum install conntrack
minikube start --driver=none